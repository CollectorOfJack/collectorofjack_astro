---
import Layout from "~/layouts/PageLayout.astro";
import Search from "astro-pagefind/components/Search";


const metadata = {
  title: "Jack Daniel's Bottle Search "
};
---
<Layout metadata={metadata}>
  <section class="px-6 sm:px-6 py-12 sm:py-16 lg:py-20 mx-auto max-w-4xl" data-pagefind-ignore="all">
    <p>Use our search engine below to find content related to your Jack Daniel's bottle search. We hope you find the site useful, if you have any feedback please sure to reach out!</p>
    <br />
    <hr/>
  
    <br />
    <h1>Search Collector Of Jack</h1>
    <Search id="search" className="pagefind-ui" uiOptions={{ showImages: false }} />
  </section>
</Layout>